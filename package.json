{
  "name": "health-logger-rn",
  "description": "iOS app for tracking medications and health data, replacing Google Forms/Sheets with a native experience",
  "main": "expo-router/entry",
  "version": "0.1.25",
  "scripts": {
    "set-variant:development": "APP_VARIANT=development && echo \"APP_VARIANT is $APP_VARIANT\"",
    "set-variant:preview": "APP_VARIANT=preview && echo \"APP_VARIANT is $APP_VARIANT\"",
    "set-variant:production": "APP_VARIANT=production && echo \"APP_VARIANT is $APP_VARIANT\"",
    "start": "APP_VARIANT=development expo start",
    "start:clear": "bun run start -- --clear",
    "start:ios": "APP_VARIANT=development expo start --ios",
    "start:android": "APP_VARIANT=development expo start --android",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "APP_VARIANT=development expo run:android",
    "ios": "APP_VARIANT=development expo run:ios",
    "ios:no-build-cache": "bun run ios -- --no-build-cache",
    "ios:clean": "bun run clean:all && bun install && bun run ios -- --no-build-cache",
    "ios:device": "bun run ios -- --device",
    "web": "bun run start -- --web",
    "prebuild:clean": "expo prebuild --clean --platform ios",
    "build": "bun run build:ios:device",
    "build:ios": "bun run prebuild:clean && expo run:ios",
    "build:ios:device": "bun run prebuild:clean && expo run:ios --device",
    "build:ios:device:release": "bun run prebuild:clean && expo run:ios --device --configuration Release",
    "build:development": "bun run version:bump-for-build && APP_VARIANT=development bun run build:ios:device",
    "build:development:no-device": "APP_VARIANT=development bun run build:ios",
    "build:development:thor": "bun run version:bump-for-build && APP_VARIANT=development bun run build:ios -- --device 1CE091A0-4A93-4780-9FBA-EDF40985CB56",
    "build:development:jphone": "bun run version:bump-for-build && APP_VARIANT=development bun run build:ios -- --device 00008140-001C2D2814A3001C",
    "build:preview": "bun run version:bump-for-build && APP_VARIANT=preview bun run build:ios:device:release",
    "build:production": "bun run version:bump-for-build && APP_VARIANT=production bun run build:ios:device:release",
    "build:eas:base": "bun run version:bump-for-build && bun run prebuild:clean && eas build --platform ios",
    "build:eas:development": "APP_VARIANT=development bun run build:eas:base -- --profile development",
    "build:eas:preview": "APP_VARIANT=preview bun run build:eas:base -- --profile preview",
    "build:eas:production": "APP_VARIANT=production bun run build:eas:base -- --profile production",
    "eas:update:preview": "bun run set-variant:preview && bun run version:bump && eas update --environment preview --channel preview --platform ios",
    "generate-fingerprint": "bun run generate-fingerprint:production",
    "generate-fingerprint:base": "eas fingerprint:generate",
    "generate-fingerprint:production": "bun run generate-fingerprint:base -- --environment production",
    "generate-fingerprint:preview": "bun run set-variant:preview && bun run generate-fingerprint:base -- --build-profile preview",
    "test": "jest --watchAll",
    "clean:all": "bun run clean:expo-native && bun run clean:node-modules",
    "clean:bundler-caches": "bun run clean:node-modules && bun install && watchman watch-del-all && rm -fr $TMPDIR/haste-map* && rm -rf $TMPDIR/metro-cache && expo start --clear",
    "clean:node-modules": "rm -rf node_modules/",
    "clean:expo-native": "rm -rf ios/ android/",
    "lint-base": "eslint --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "bun run lint-base -- --fix .",
    "lint:fix:file": "bun run lint-base -- --fix",
    "lint": "bun run lint-base -- .",
    "lint:no-cache": "bun run lint-base -- --no-cache .",
    "prettier-check": "prettier --check .",
    "prettier": "prettier --write .",
    "prettier:write:file": "prettier --write --ignore-unknown",
    "signal:serial": "bun run ts-check; bun run lint; bun run prettier-check",
    "signal": "bun run signal:serial",
    "signal:parallel": "concurrently --names \"TS,LINT,PRETTIER\" -c \"bgBlack.bold,bgRed.bold,bgCyan.bold\" \"bun run ts-check\" \"bun run lint\" \"bun run prettier-check\"",
    "ts-check": "tsc --noEmit",
    "ts-check-masked": "bun run ts-check",
    "generate-screenshots": "maestro test e2e/screenshot_flows/*.yaml",
    "test-e2e:home": "bun e2e/scripts/run-screenshot-tests.ts --flows home",
    "test-e2e:all": "bun e2e/scripts/run-screenshot-tests.ts",
    "test-e2e:skip-install": "bun e2e/scripts/run-screenshot-tests.ts --skip-install",
    "test-e2e:light": "bun e2e/scripts/run-screenshot-tests.ts --themes light",
    "test-e2e:dark": "bun e2e/scripts/run-screenshot-tests.ts --themes dark",
    "test-e2e:production": "bun e2e/scripts/run-screenshot-tests.ts",
    "test-e2e:list-devices": "bun e2e/scripts/list-simulators.ts",
    "e2e:setup-simulator": "bun e2e/scripts/setup-e2e-simulator.ts",
    "archive-screenshots": "ts-node e2e/scripts/archive-screenshots.ts",
    "open-xcode": "xed ios",
    "prepare": "husky",
    "reinstall-my-lint-config": "bun add eslint-config-jha-react-node@github:justinhaaheim/eslint-config-jha-react-node",
    "install-my-prompts": "bunx git+https://github.com/justinhaaheim/prompts --target-dir docs/prompts --md",
    "version": "bun scripts/version-manager.ts",
    "version:bump": "bun scripts/version-manager.ts --bump",
    "version:bump:major": "bun scripts/version-manager.ts --bump --major",
    "version:bump:minor": "bun scripts/version-manager.ts --bump --minor",
    "version:bump-for-build": "bun scripts/version-manager.ts --bump-for-build",
    "version:build-only": "bun scripts/version-manager.ts --build-only",
    "version:update-runtime": "bun scripts/version-manager.ts --bump --minor --update-runtime",
    "version:interactive": "bun scripts/version-manager.ts",
    "version:show": "bun scripts/version-manager.ts --show",
    "version:history": "bun scripts/version-manager.ts --history",
    "version:repair": "bun scripts/version-manager.ts --repair"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/fingerprint": "0.13.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-google-signin/google-signin": "15.0.0",
    "@react-navigation/native": "^7.1.6",
    "@tamagui/babel-plugin": "1.132.20",
    "@tamagui/colors": "1.132.20",
    "@tamagui/config": "1.132.20",
    "@tamagui/lucide-icons": "1.132.20",
    "@tamagui/metro-plugin": "1.132.20",
    "@tamagui/theme-builder": "1.132.20",
    "@tamagui/toast": "1.132.20",
    "@tanstack/react-query": "5.84.2",
    "@types/eventemitter3": "^1.2.0",
    "@types/js-yaml": "4.0.9",
    "@types/lodash.debounce": "4.0.9",
    "@types/semver": "7.7.0",
    "@types/uuid": "^10.0.0",
    "axios": "1.11.0",
    "burnt": "^0.13.0",
    "dayjs": "1.11.13",
    "eventemitter3": "^5.0.1",
    "expo": "^53.0.20",
    "expo-application": "~6.1.5",
    "expo-auth-session": "6.2.1",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.4",
    "expo-secure-store": "14.2.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-updates": "0.28.17",
    "expo-web-browser": "~14.2.0",
    "js-yaml": "4.1.0",
    "lodash.debounce": "4.0.8",
    "nullthrows": "1.1.1",
    "path-browserify": "^1.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-launch-arguments": "^4.1.0",
    "react-native-reanimated": "3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "semver": "7.7.2",
    "tamagui": "1.132.20",
    "uuid": "^11.1.0",
    "zustand": "5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.12",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^19.1.8",
    "@types/react-test-renderer": "^18.3.0",
    "@types/yargs": "^17.0.33",
    "babel-plugin-inline-import": "^3.0.0",
    "concurrently": "8.2.1",
    "dotenv": "^16.5.0",
    "eslint": "9.31.0",
    "eslint-config-expo": "9.2.0",
    "eslint-config-jha-react-node": "github:justinhaaheim/eslint-config-jha-react-node",
    "husky": "9.1.7",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.9",
    "lint-staged": "^14.0.1",
    "prettier": "3.6.2",
    "react-test-renderer": "19.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "yargs": "^18.0.0",
    "zod": "4.1.5"
  },
  "private": true,
  "lint-staged": {
    "**/*": "prettier --check --ignore-unknown",
    "*.(js|jsx|cjs|ts|tsx)": [
      "eslint --cache --max-warnings=0 --no-warn-ignored"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
